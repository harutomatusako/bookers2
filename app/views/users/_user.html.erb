
     <h1>UserInfo</h1>

     <%= attachment_image_tag user, :profile_image, :fill, 100, 100, format: 'jpeg',fallback:"no_image.jpg" %>
     <table class="table">
      <tbody>
       <tr>
        <th>name</th>
        <th><%= user.name %></th>
       </tr>
       <tr>
        <th>introduction</th>
        <th><%= user.introduction %></th>
       </tr>
      </tbody>
     </table>
     <div class="fas fa-user-cog" >
      <%= link_to "edit",edit_user_path(user.id) %>
      </div>
